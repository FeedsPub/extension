{
  "manifest_version": 2,
  "author": "timqian",
  "homepage_url": "https://feeds.pub",
  "name": "Feeds Pub",
  "description": "An RSS aggregator and network",
  "version": "1.3",
  "icons": {
    "128": "assets/icon.png"
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "notifications",
    "background",
    "alarms"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
